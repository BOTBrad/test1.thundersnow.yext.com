{
    "configVersion": "1",
    "deviceDetected": false,
    "https": false,
    "liveSite": false,
    "locales": ["en"],
    "templateVersion": 6,
    "features": [
	{
	    "name": "Locations",
	    "locationPageSet": {
		"urlTemplate": "url.location",
		"template": "desktop.location.default",
		"outputFormat": ["Html", "Json"],
                "locationScope" : {
                    "savedSearch" : "NY"
                }
	    }
	},
	{
	    "name": "Location Directory",
	    "locationDirectory": {
		"levels": [
		    {
			"urlTemplate": "url.directoryStateList",
			"template": "desktop.directory.stateList",
			"field": "State",
			"orderBy": {
			    "field": "State",
			    "order": "ascending"
			}
		    },
		    {
			"urlTemplate": "url.directoryCityList",
			"template": "desktop.directory.cityList",
			"field": "City",
			"orderBy": {
			    "field": "City",
			    "order": "ascending"
			}
		    },
		    {
			"urlTemplate": "url.directoryLocationList",
			"template": "desktop.directory.locationList",
			"field": "Locations",
			"orderBy": {
			    "field": "Locations",
			    "order": "ascending"
			}
		    }
		],
		"locationPageSet": "Locations",
		"outputFormat": ["Html", "Json"]
	    }
	},
        {
            "name": "Search",
            "search": {
                "locationScopeOverride" : {
                    "savedSearch" : "NY"
                },
                "urlTemplate": "url.search",
                "template": "desktop.search.default",
                "queryParam": "q",
                "disableReverseIpSearch": false,
                "maxRadiusParam": {
                    "name": "r",
                    "default": 50
                },
                "pageResultParam": {
                    "name": "per",
                    "default": 50
                },
                "pageToShowParam": "p",
                "totalResultParam": {
                    "name": "max",
                    "default": 50
                },
                "searchFields": []
            }
        }
    ]
}
